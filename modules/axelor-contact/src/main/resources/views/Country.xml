<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://axelor.com/xml/ns/object-views https://axelor.com/xml/ns/object-views/object-views_6.0.xsd">

    <grid name="country-grid"
          title="Countries"
          model="com.axelor.apps.contact.db.Country">
        <field name="code" title="Code" required="true" min="2"/>
        <field name="name" title="Name" required="true" min="2"/>

    </grid>

    <form name="country-form"
          title="Countries"
          model="com.axelor.apps.contact.db.Country">

        <panel name="overviewPanel" title="Country Overview">

            <field name="code" title="Code" required="true" min="2"/>
            <field name="name" title="Name" required="true" min="2"/>

        </panel>
    </form>



</object-views>